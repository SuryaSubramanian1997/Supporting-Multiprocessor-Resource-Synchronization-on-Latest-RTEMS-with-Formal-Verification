+    case SEMAPHORE_VARIANT_FMLPS:
+      status = RTEMS_NOT_DEFINED;
+      break;
+    case SEMAPHORE_VARIANT_FMLPL:
+      status = RTEMS_NOT_DEFINED;
+      break;
+    case SEMAPHORE_VARIANT_DFLPL:
+      _DFLPL_Set_CPU(
+        &the_semaphore->Core_control.DFLPL,
